{
  "name": "tracing-test",
  "version": "1.0.0",
  "description": "Test tracing on Azure",
  "main": "dist/functions.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "node --require ./dist/instrumentation.js dist/app-service.js",
    "start": "node dist/app-service.js",
    "dev": "node --env-file=.env dist/app-service.js",
